<%- include('cabecalho') %>
<form action="/idade" method="post">
    <label>
        <span>Nome</span>
        <input name="nome" />
    </label>
    <label>
        <span>Ano</span>
        <input name="ano" type="number" />
    </label>
    <button>Enviar</button>
</form>

<section id="servicos">
    <h2>Nossos servi√ßos</h2>
<%
for (let servico of servicos) {
_%>
    <article class="servico">
        <h3><%= servico.nome %></h3>
        <img src="<%= servico.imagem %>" alt="<%= servico.nome %>">
        <span><%= servico.duracao %>h</span>
        <span><%= servico.preco %></span>
        <a href="/detalhes/<%= servico.id %>">Ver mais</a>
    </article>
<%
}
_%>
</section>